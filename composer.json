{
    "name": "lucasleandrobr/laravel-asaas",
    "description": "Asaas Integration for laravel 10+",
    "version": "1.1.0",
    "keywords": [
        "lucasleandrobr",
        "laravel-asaas"
    ],
    "homepage": "https://github.com/lucasleandrobr/laravel-asaas",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lucas L. S. Melo",
            "email": "lucasleandro380@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LucasLeandroBR\\LaravelAsaas\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LucasLeandroBR\\LaravelAsaas\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LucasLeandroBR\\LaravelAsaas\\LaravelAsaasServiceProvider"
            ],
            "aliases": {
                "LaravelAsaas": "LucasLeandroBR\\LaravelAsaas\\LaravelAsaasFacade"
            }
        }
    }
}
